{
  "expo": {
    "name": "CoachdiaryMobile",
    "slug": "CoachdiaryMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whistleblower_icon.png",
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ru.coachdiary",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/whistleblower_icon.png",
        "backgroundColor": "#1c1c1c"
      },
      "package": "com.sobol_mobol.CoachdiaryMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mobile_logo_blue.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ebaac76c-4d8e-4c09-8cc8-3f3f00b21d97"
      }
    }
  }
}
